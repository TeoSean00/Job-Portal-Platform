{
  "name": "is212-spm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pipenv install --dev && pipenv run uvicorn api.main:app --reload",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "tsc && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "npx eslint -c .eslintrc.js . --fix",
    "tsc": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "jest --testPathPattern={app,tests} --passWithNoTests",
    "test:watch": "jest --testPathPattern={app,tests} --watch",
    "lint:markdown": "npx remark --frail .",
    "lint:markdown:fix": "npx remark . -qfo"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "20.5.2",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.1",
    "eslint-config-next": "13.4.19",
    "jest": "^29.7.0",
    "lucide-react": "^0.279.0",
    "next": "13.4.19",
    "postcss": "8.4.28",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark-cli": "^11.0.0",
    "remark-preset-lint-consistent": "^5.1.1",
    "remark-preset-lint-recommended": "^6.1.2",
    "sass": "^1.66.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.1.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "typescript": "^5.1.6"
  }
}
